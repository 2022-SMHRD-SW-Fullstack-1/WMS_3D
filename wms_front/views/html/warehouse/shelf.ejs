          <div class="search_div">
            <div class="dropdown">
              <button onclick="dp_menu()" class="button">선반명 ▼</button>
              <div style="display: none" id="drop-content">
                <a href="#">선반 명</a>
                <a href="#">선반 너비 / 길이</a>
                <a href="#">선반 총 수</a>
                <a href="#">적재 가능 / 총 공간</a>
              </div>
            </div>
            <input type="text" />
            <button id="search_btn">검색</button>
          </div>
          <button id="add_warehouse_btn" onclick="goToCreateShelf('<%- data[0].wh_num%>')">+ 선반 추가</button>
        </div>
        
        <div class="graph_top_div">
          <div class="main_graph_div">
            <div class="bd-example">
              <table class="table table-striped table-hover">
                <thead>
                  <tr>
                    <th><input type="checkbox" /></th>
                    <th scope="col">선반 명</th>
                    <th scope="col">선반 너비 / 길이</th>
                    <th scope="col">선반 총 수</th>
                  </tr>
                </thead>
                <tbody>

                  <% for(var i = 0; i<data.length; i++){%>
                      <td><input type="checkbox" /></td>
                      <td><%- data[i].shelf_name%></td>
                      <td><%- data[i].shelf_width %>   /   <%- data[i].shelf_length %></td>
                      <td><%- data[i].shelf_floor %>  </td>
                      <td></td>
                      <td>
                        <div
                        class="modal fade"
                        id="exampleModalCenteredScrollable<%- i%>"
                        tabindex="-1"
                          aria-labelledby="exampleModalCenteredScrollableTitle"
                          aria-hidden="true"
                        >
                          <div
                            class="modal-dialog modal-dialog-centered modal-dialog-scrollable"
                          >
                            <div class="modal-content">
                              <div class="modal-header">
                                <h1
                                  class="modal-title fs-5"
                                  id="exampleModalCenteredScrollableTitle"
                                >
                                  선반 상세 정보
                                </h1>
                                <button
                                  type="button"
                                  class="btn-close"
                                  data-bs-dismiss="modal"
                                  aria-label="Close"
                                ></button>
                              </div>
                              <div class="modal-body">
                                <div>
                                  <span>선반 명 : </span><span><%- data[i].shelf_name%></span>
                                </div>
                                <div>
                                  <span>선반 너비 : </span><span><%- data[i].shelf_width%> m</span>
                                </div>
                                <div>
                                  <span>선반 길이 : </span><span><%- data[i].shelf_length%> m</span>
                                </div>
                                <div><span>선반 층수 : </span><span><%- data[i].shelf_floor%> 층</span>
                                </div>
                                
                              </div>
                              <div class="modal-footer">
                                <button id="add_warehouse_btn" onclick="goToWarehouse()">
                                        선반위치보기
                                </button>
                                </div>
                            </div>
                          </div>
                        </div>
                        <div class="bd-example">
                          <button
                            type="button"
                            data-bs-toggle="modal"
                            data-bs-target="#exampleModalCenteredScrollable<%- i%>"
                          >
                            상세보기
                          </button>
                        </div>
                      </td>
                    </tr>
                  <% } %>  


                </tbody>
              </table>

              <!-- 페이지 바 -->
              <nav aria-label="Page navigation example">
                <ul class="pagination justify-content-center">
                  <li class="page-item disabled">
                    <a class="page-link">Previous</a>
