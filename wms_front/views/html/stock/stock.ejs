          </div>
        </ul>
      </div>

<!-- == 사이드바 -->
<div class="main_div">
  <div class="management_div">
    <a href="#">재고 현황</a>
  </div>
  <hr />
  <div class="btn_div">
    <div>
      <button>수정</button>
      <button onclick="stockOutput()">출고</button>
    </div>

    <div></div>

    <div class="search_div">
      <div class="dropdown">
        <button onclick="dp_menu()" class="button">재고 명 ▼</button>
        <div style="display: none" id="drop-content">
          <a href="#">재고 번호</a>
          <a href="#">재고 명</a>
          <a href="#">재고 정보</a>
          <a href="#">구매처 명</a>
          <a href="#">창고 명</a>
          <a href="#">소비기한</a>
        </div>
      </div>
      <input type="text" />
      <button id="search_btn">검색</button>
    </div>
  </div>
  
  <div class="graph_top_div">
    <div class="main_graph_div">
      <div class="bd-example">
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th><input type="checkbox" /></th>
              <th scope="col">재고 번호</th>
              <th scope="col">재고 명</th>
              <th scope="col">재고 정보</th>
              <th scope="col">구매처 명</th>
              <th scope="col">창고 명</th>
              <th scope="col">소비기한</th>
              <th></th>
            </tr>
          </thead>
          <tbody>

            <%for(var i = 0; i<data.length; i++){%>
              <tr>
                <td><input type="checkbox" id="my_checkbox" onclick="isChecked('<%- data[i].stock_num%>')"/></td>
                <td><%- data[i].stock_num%></td>
                <td><%- data[i].stock_name%></td>
                <td><%- data[i].stock_info%></td>
                <td><%- data[i].buy_com%></td>
                <td><%- data[i].wh_name%></td>
                <td><%- data[i].exp_dt%></td>
                <td>
                  <button class = "btn_small" onclick="goToWarehouse({wh_num:'<%- data[i].wh_num%>',stock_num:'<%- data[i].stock_num%>'})">위치확인</button>
                </td>
              </tr>
            <% } %>

          </tbody>
        </table>
      </div>
    </div>
    <!-- <% for(var i = 0; i<data.length; i++){%>
      <p><%- data[i].wh_num%></p>
  <%}%> -->
  </div>
</div>
</div>
